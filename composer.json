{
  "name": "craigh/imap-cache-manager",
  "description": "Provides caching support for imap-mail-manager",
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Craig Humphreys",
      "email": "craig_h_411@yahoo.co.uk"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^5.1",
    "sami/sami": "~3.0.0",
    "mockery/mockery": "^0.9.4"
  },
  "require": {
    "craigh/imap-mail-manager": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Humps\\ImapCacheManager\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Humps\\ImapCacheManager\\Tests\\": "tests"
    }
  },
  "scripts": {
    "docs": "vendor\\bin\\sami.php.bat update sami.php -v",
    "tests": "vendor\\bin\\phpunit tests -c tests/phpunit.xml"
  }
}
